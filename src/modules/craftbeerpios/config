# CraftBeerOS repo & branch
[ -n "$CRAFTBEERPIOS_REPO_SHIP" ] || CRAFTBEERPIOS_REPO_SHIP=https://github.com/Manuel83/craftbeerpi.git
[ -n "$CRAFTBEERPIOS_REPO_BUILD" ] || CRAFTBEERPIOS_REPO_BUILD=
[ -n "$CRAFTBEERPIOS_REPO_BRANCH" ] || CRAFTBEERPIOS_REPO_BRANCH=master


#CraftBeerBot repo & branch
[ -n "$CRAFTBEERPIOS_TELEGRAM_REPO_SHIP" ] || CRAFTBEERPIOS_TELEGRAM_REPO_SHIP=https://github.com/guysoft/CraftBeerPiBot.git
[ -n "$CRAFTBEERPIOS_TELEGRAM_REPO_BUILD" ] || CRAFTBEERPIOS_TELEGRAM_REPO_BUILD=
[ -n "$CRAFTBEERPIOS_TELEGRAM_REPO_BRANCH" ] || CRAFTBEERPIOS_TELEGRAM_REPO_BRANCH=master
[ -n "$CRAFTBEERPIOS_INCLUDE_TELEGRAM" ] || CRAFTBEERPIOS_INCLUDE_TELEGRAM=yes
